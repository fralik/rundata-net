{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug uvu Tests",
        "program": "${workspaceFolder}/node_modules/uvu/bin.js",
        "args": ["${workspaceFolder}/rundatanet/tests/js"],
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "skipFiles": ["<node_internals>/**"]
      },
      {
        "type": "node",
        "request": "launch",
        "name": "Debug Current uvu Test",
        "program": "${workspaceFolder}/node_modules/uvu/bin.js",
        "args": ["${relativeFileDirname}", "${fileBasenameNoExtension}"],
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "skipFiles": ["<node_internals>/**"]
      }
    ]
  }